<?xml version="1.0" encoding="UTF-8"?>

<mule xmlns:customer-document-management="http://www.mulesoft.org/schema/mule/customer-document-management"
	xmlns:ee="http://www.mulesoft.org/schema/mule/ee/core" xmlns="http://www.mulesoft.org/schema/mule/core"
	xmlns:doc="http://www.mulesoft.org/schema/mule/documentation"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="
http://www.mulesoft.org/schema/mule/ee/core http://www.mulesoft.org/schema/mule/ee/core/current/mule-ee.xsd http://www.mulesoft.org/schema/mule/core http://www.mulesoft.org/schema/mule/core/current/mule.xsd
http://www.mulesoft.org/schema/mule/customer-document-management http://www.mulesoft.org/schema/mule/customer-document-management/current/mule-customer-document-management.xsd">
	<flow name="cheque-images-implementationFlow" doc:id="ed3456ab-ea64-455c-a7e1-511d53656357">
	
		<logger level="INFO" doc:name=" Before get cheque images Logger" doc:id="a0df7593-e25e-4729-bad1-d8e236f20b61" message='#[" Before cheque images Logger"]' />
		<set-variable value='#[attributes.queryParams.date as String {format: "yyyy-MM-dd"}]' doc:name="Date Var" doc:id="ca539ffc-0823-40c9-9c32-ea46c7acd277" variableName="date"/>
		<logger level="INFO" doc:name="Logger" doc:id="f1a397be-3e16-4f3c-8f84-39a0d33ab170" message="#[vars.date]"/>
		<customer-document-management:get-on-us-cheque-images doc:name="Get on us cheque images" doc:id="9e7f4eea-c54b-4a6d-adf3-d5e1a6859cea" routing-number="#[attributes.queryParams.routingNumber]" transit-number="#[attributes.queryParams.transitNumber]" account-number="#[attributes.queryParams.accountNumber]" date='#[vars.date]' amount="#[attributes.queryParams.amount]" cheque-number="#[attributes.queryParams.chequeNumber]" trace-number="#[attributes.queryParams.traceNumber]" config-ref="Customer_Document_Management_Config"/>
		<ee:transform doc:name="Response of cheque-images" doc:id="56052d34-76cb-40cd-9f34-709b1c9fd7c9">
			<ee:message>
				<ee:set-payload><![CDATA[%dw 2.0
output application/java
---
{
  "frontImage": payload.frontImage,
  "backImage": payload.backImage
}
]]></ee:set-payload>
			</ee:message>
		</ee:transform>
		<logger level="INFO" doc:name=" After get cheque images  Logger" doc:id="7f9d5cbf-cc56-4560-98dd-a8babcff261e" message='#[" After get cheque images Logger"]' />
	
	</flow>
</mule>
